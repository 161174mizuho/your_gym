<div class="container">
  <div class="row">
    <h1>店舗情報編集</h1>
  </div>
  <div class="row">
    <%= form_with model: @admin do |f| %>
    <h4>メールアドレス</h4>
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
    <h4>店舗名</h4>
    <%= f.text_field :gym_name %>
    <h4>店舗画像</h4>
    <%= f.text_field :gym_name %>
    <% end %>
  </div>
</div>